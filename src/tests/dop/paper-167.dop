prec: 0.001

var:
    x1 : [0.2, 1.1]
    x2 : [0.2, 1.1]

cost:
    (100 * (x1^2 - x1)^2 + (1 - x1)^2)^2 / 4000 - cos(100 * (x1^2 - x1)^2 + (1-x1)^2 + 1) +
    (100 * (x1^2 - x2)^2 + (1 - x2)^2)^2 / 4000 - cos(100 * (x1^2 - x2)^2 + (1-x2)^2 + 1) +
    (100 * (x2^2 - x1)^2 + (1 - x1)^2)^2 / 4000 - cos(100 * (x2^2 - x1)^2 + (1-x1)^2 + 1) +
    (100 * (x2^2 - x2)^2 + (1 - x2)^2)^2 / 4000 - cos(100 * (x2^2 - x2)^2 + (1-x2)^2 + 1)

# Global minimum is located at x* = f(1.0, 1.0)
# f(x*) = 0.0
